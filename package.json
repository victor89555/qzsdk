{
  "name": "qzsdk",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "mock": "node ./build/mock-server.js",
    "pwa": "node ./build/sw-precache.js",
    "start": "npm-run-all --parallel mock serve",
    "build": "ng build --prod --aot --base-href=/rebirth-admin/ && npm run pwa",
    "serve": "ng serve --port=4200",
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint -c tslint.json \"src/**/*.ts\"  --fix",
    "test": "ng lint && ng test --watch=false --single-run=true --code-coverage",
    "ut": "ng test --watch=true --single-run=false --browsers=Chrome",
    "e2e": "ng e2e",
    "i18n:extract": "ng-xi18n",
    "build-electron": "ng build --base-href ./ && cp src/electron/* dist",
    "electron-run": "npm run build-electron && electron dist",
    "electron": "npm-run-all --parallel mock electron-run",
    "electron-pack": "npm run build-electron && electron-packager dist --out dist/electron",
    "docs:serve": "compodoc ./src/app/ -p ./src/tsconfig.app.json -b / -d ./compodocs/ --hideGenerator -s",
    "docs:gen": "compodoc ./src/app/ -p ./src/tsconfig.app.json -b /rebirth-admin/compodocs/ -d ./dist/compodocs/ --hideGenerator",
    "github:deploy": "npm run build && npm run docs:gen && ngh --message \"rebirth admin deploy\" && rm -rf ./dist",
    "prepush": "npm test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "rebirth-event-source": "^0.0.1",
    "rebirth-http": "^0.1.5",
    "rebirth-permission": "^0.0.8",
    "rebirth-storage": "^0.0.11",
    "rxjs": "^5.5.2",
    "weui": "^1.1.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.0.25",
    "@types/protractor": "^4.0.0",
    "@types/webpack": "^3.0.10",
    "angular-cli-ghpages": "^0.5.0",
    "codelyzer": "^4.0.1",
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "gulp-stubby-server": "^0.2.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "sw-precache": "^5.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": ">=2.4.2 <2.5",
    "npm-run-all": "^4.1.1",
    "stubby": "^4.0.0"
  }
}
